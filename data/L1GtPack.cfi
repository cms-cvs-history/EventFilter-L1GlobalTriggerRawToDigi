module L1GtPack = L1GTDigiToRaw {
 
    // input tag for GT readout collection: 
    //     L1GtEmul = GT emulator, 
    //     L1GtUnpack = GT unpacker 
    untracked InputTag DaqGtInputTag = L1GtEmul
    
    /// mask for active boards (actually 16 bits)
    //      if bit is zero, the corresponding board will not be packed
    //      default: all boards active
    uint32 ActiveBoardsMask = 0xFFFF


}
